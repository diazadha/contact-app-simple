
    <h1>Hello World</h1>
    <p>Welcome to my site..</p>
   
    <h2>Halo <%= nama %></h2>
    <h4>Daftar Mahasiswa</h4>

    <% if (mahasiswa.length === 0) { %>
     <p>Data mahasiswa masih kosong</p>
    <% } else { %>
    <% mahasiswa.forEach(v => { %>
        <ul>
            <li>
                nama : <%= v.nama %>
            </li>
            <li>
                email : <%= v.email %>
            </li>
        </ul>
    <% }) %>
    <% } %> 
